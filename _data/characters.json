[
{% for character in site.characters %}
  {
    "title": "{{ character.title }}",
    "image": "{{ character.image | default: 'https://via.placeholder.com/420' }}",
    "bio": "{{ character.bio | default: 'No bio available.' }}",
    "url": "{{ character.url | relative_url }}"
  }{% if forloop.last == false %},{% endif %}
{% endfor %}
]
