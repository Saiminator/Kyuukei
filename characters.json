[
{% for character in site.characters %}
  {
    "title": "{{ character.title | escape }}",
    "image": "{{ character.image | relative_url }}",
    "url": "{{ character.url | relative_url }}",
    "last_modified_at": "{{ character.last_modified_at }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
